<%= form_for(@quiz) do |f|%>
  <div class="form-group mx-2">
    <%= f.label :title %>
    <%= f.text_field :title, class: "form-control w-50" %>
  </div>
  <div class="form-group mx-2">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control w-50" %>
  </div>
  <%= f.submit class: "btn btn-outline-primary mx-2" %>
<% end %>
